ow.location.pathname);\r\n    };\r\n\r\n    window.addEventListener('popstate',onLocationChange);\r\n\r\n    return () => {\r\n        window.removeEventListener('popstate',onLocationChange);\r\n    }\r\n},[]);\r\n\r\n    return currentPath === path\r\n    ? children\r\n    : null ;\r\n};\r\n\r\nexport default Route;","C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\Header.jsx",[],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\Link.jsx",[],["136","137"],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\Home.jsx",["138"],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\Logout.jsx",[],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\Spinner.jsx",[],["139","140"],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\AddBook.jsx",[],"C:\\Users\\Samer\\Core\\Desktop\\reg\\modern-react-login-master\\src\\components\\pages\\AllBooks.jsx",["141","142","143","144","145"],"import React from \"react\";\r\nimport './style.css';\r\nimport BookPage from \"./BookPage\";\r\nimport Footer from \"./Footer\";\r\nimport Link from \"./Link\";\r\nlet higherPurchaseArrayHelper = [];\r\nlet xy = [];\r\nlet xyz = [];\r\nconst shuffle = (a) => {\r\n    let j, x, i;\r\n    for (i = a.length - 1; i > 0; i--) {\r\n        j = Math.floor(Math.random() * (i + 1));\r\n        x = a[i];\r\n        a[i] = a[j];\r\n        a[j] = x;\r\n    }\r\n    return a;\r\n}\r\n\r\nconst AllBooks = ({ account, accounts, allBooks }) => {\r\n    const [selectedBook, setSelectedBook] = React.useState(null);\r\n    const [selectedBookNewest, setSelectedBookNewest] = React.useState(null);\r\n    const [selectedBookAllBooksRandom, setSelectedBookAllBooksRandom] = React.useState(null);\r\n    const [higherPurchase, setHigherPurchase] = React.useState([]);\r\n    const [shuffleAll, 